{
  "name": "carehome_backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f dev.env env-cmd -f local.env ts-node-dev -r tsconfig-paths/register --notify=false --rs src/server.ts",
    "prod": "env-cmd -f prod.env ts-node-dev -r tsconfig-paths/register --notify=false --rs src/server.ts",
    "start": "env-cmd -f dev.env node dist/index.js",
    "start:live": "env-cmd -f prod.env node dist/index.js",
    "build": "rm -rf dist && webpack --mode development",
    "build:live": "rm -rf dist && webpack --mode production",
    "deploy": "npm run build && npm run start",
    "deploy:live": "npm run build:live && npm run start:live",
    "format": "prettier --write \"**/*.{ts,tsx}\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "aws-sdk": "^2.1573.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.3",
    "fcm-node": "^1.3.0",
    "firebase-admin": "^12.0.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.2.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.11",
    "nodemon-webpack-plugin": "^4.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
